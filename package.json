{
  "name": "site-starter",
  "version": "0.4.0",
  "description": "Build a site with Webpack, Babel, TypeScript or ES6, critical CSS, differential loading & Web Components",
  "scripts": {
    "start": "webpack-dev-server --open --config webpack.config.dev.js",
    "build": "node build.js",
    "test": "concurrently \"webpack-dev-server --config webpack.config.dev.js\" \"cypress open\"",
    "test:ci": "cypress run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/robisim74/site-starter"
  },
  "bugs": {
    "url": "https://github.com/robisim74/site-starter/issues"
  },
  "homepage": "https://github.com/robisim74/site-starter",
  "keywords": [
    "webpack",
    "babel",
    "es6",
    "typescript",
    "critical css",
    "differential loading",
    "multi page",
    "web components",
    "site",
    "starter"
  ],
  "author": "Roberto Simonetti",
  "license": "MIT",
  "devDependencies": {
